<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewPlatformController">
   <children>
      <Label layoutX="217.0" layoutY="14.0" text="View Platform">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="trainIdTextfield" layoutX="14.0" layoutY="70.0" promptText="Train ID" />
      <Button fx:id="checkPlatform" layoutX="200.0" layoutY="70.0" mnemonicParsing="false" onAction="#sendRequest" text="Check Platform" />
      <Hyperlink fx:id="homeLink" layoutX="509.0" layoutY="374.0" onAction="#goToHome" text="Go to home." />
      <ListView fx:id="holder" layoutY="113.0" prefHeight="260.0" prefWidth="600.0" />
   </children>
</AnchorPane>
